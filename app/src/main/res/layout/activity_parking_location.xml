<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ui.ParkingLocationActivity">

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tabLayout"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.060000002">

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mobil"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="123dp" />

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Motor"
            tools:layout_editor_absoluteX="93dp"
            tools:layout_editor_absoluteY="325dp" />

    </com.google.android.material.tabs.TabLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_back2"
        android:layout_width="65dp"
        android:layout_height="41dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="36dp"

        android:background="#00FFFFFF"
        app:icon="@drawable/baseline_arrow_back_ios_new_24"
        app:iconTint="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Cari Parkir"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_back2"
        app:layout_constraintStart_toEndOf="@+id/button_back2"
        app:layout_constraintTop_toTopOf="@+id/button_back2" />

    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/viewPager_parking"
        android:layout_width="410dp"
        android:layout_height="570dp"
        android:layout_marginTop="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tabLayout"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>