<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ui.ProfileActivity">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/shapeableImageView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_margin="10dp"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246"
        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.CornerSize50Percent"
        app:srcCompat="@mipmap/ic_launcher"
        app:strokeColor="@android:color/darker_gray"
        app:strokeWidth="5dp" />

    <com.google.android.material.button.MaterialButton
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:background="transparent"
        android:text="Kembali"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        app:icon="@drawable/baseline_arrow_back_ios_new_24"
        app:iconTint="@color/black"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="16dp"
        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rauuf Agrakb"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="380dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="rauufakbar03@gmail.com"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="409dp" />

    <com.google.android.material.button.MaterialButton
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="transparent"
        android:text="Sign Out"
        android:textColor="@color/quantum_googred800"
        app:icon="@drawable/baseline_exit_to_app_24"
        app:iconTint="@color/quantum_googred900"
        app:iconGravity="textEnd"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="523dp" />
</androidx.constraintlayout.widget.ConstraintLayout>